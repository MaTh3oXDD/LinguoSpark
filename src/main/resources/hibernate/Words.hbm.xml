<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.example.Word" table="words">
        <id name="id" column="id">
            <generator class="identity"/>
        </id>
        <property name="polish" column="pol" type="string"/>
        <property name="english" column="ang" type="string"/>
        <many-to-one name="category" class="com.example.Category" fetch="select">
            <column name="category_id" not-null="true"/>
        </many-to-one>
    </class>
</hibernate-mapping>
